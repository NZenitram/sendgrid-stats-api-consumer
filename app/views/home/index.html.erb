<div class="form">
  <%= form_tag("/dates", method: "post", class: 'client-form') do %>
    Client ID: <li class="li-form client-id"><%= text_field_tag('client-id') %></li>
    Start Date: <li class="li-form form-pad"><%= text_field_tag('datepicker-start', nil, placeholder: 'mm/dd/yyyy') %></li>
    End Date: <li class="li-form form-pad"><%= text_field_tag('datepicker-end', nil, placeholder: 'mm/dd/yyyy') %></li>

  <li class="li-form form-sub form-pad"><%= submit_tag("Fetch!") %></li>
  <% end  %>
</div>

<div id="provider-side-bar" class="inline-display">
  <ul id="side-provider">
    <li class="prov-links"><a href="/top-five.html">Top Five Inboxes</a></li>
    <li class="prov-links"> <a href="/providers.html">View Providers</a></li>
    <li class="prov-links"></li>
  </ul>
</div>

<div>
  <div id="container" class="inline-display graph-data" style="min-width: 310px; height: 400px; margin: 0 auto">
  </div>

  <div id="events-blocks">
    <div id="spam">
      <div id="percentages-label">
        <span class="percentages-text">Spam %</span>
      </div>
      <div id="percentages-number">
        <span class="percentages-num">.2%</span>
      </div>
    </div>
    <div id="opens">
      <div id="percentages-label">
        <span class="percentages-text">Open %</span>
      </div>
      <div id="percentages-number">
        <span class="percentages-num">.2%</span>
      </div>
    </div>
    <div id="clicks">
      <div id="percentages-label">
        <span class="percentages-text">Click %</span>
      </div>
      <div id="percentages-number">
        <span class="percentages-num">.2%</span>
      </div>
    </div>
  </div>
</div>
