<div id="provider-side-bar" class="inline-display">
  <ul id="side-provider">

    <% @providers.each do |provider| %>
    <li class="prov-links"><%= p provider %><input class="providerCheckBox" type="checkbox" name="chk[]"></li>
    <% end %>
  </ul>
</div>

<div id="providers-page-header">
  <div id="providers-title">
    <h1>Inbox Statistics By Provider</h1>
    <p>(Select domains on the left)</p>
  </div>
  <div id="provider-button" class="multiple-providers">
    <p>Get Providers</p>
  </div>
  <div id="percentage-button" class="multiple-providers-percent">
    <p>Get Percentages</p>
  </div>
  <div id="clear-button">
    <p>Clear</p>
  </div>
</div>

<div id="graphs">
</div>
